{
  "name": "@voxpelli/tstyche-reporters",
  "version": "0.0.0",
  "description": "Custom TStyche reporters with Mocha-style and dot-style output, featuring CI-aware Markdown/CLI dual-mode formatting",
  "homepage": "https://github.com/voxpelli/tstyche-reporters",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/tstyche-reporters.git"
  },
  "keywords": [
    "tstyche",
    "reporter",
    "typescript",
    "type-testing",
    "mocha",
    "dot-reporter",
    "ci",
    "github-actions",
    "markdown"
  ],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": ">=22.12.0",
    "typescript": ">=5.5.0"
  },
  "type": "module",
  "exports": "./index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "index.d.ts.map",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.d.ts.map"
  ],
  "scripts": {
    "build:0": "run-s clean",
    "build:1-declaration": "tsc -p declaration.tsconfig.json",
    "build": "run-s build:*",
    "check:installed-check": "installed-check",
    "check:knip": "knip",
    "check:lint": "eslint",
    "check:tsc": "tsc",
    "check:type-coverage": "type-coverage --detail --strict --at-least 99 --ignore-files 'test/*'",
    "check": "run-s clean && run-p check:*",
    "clean:declarations-top": "rm -f $(find . -maxdepth 1 -type f -name '*.d.ts*' ! -name 'index.d.ts')",
    "clean:declarations-lib": "rm -f $(find lib -type f -name '*.d.ts*' ! -name '*-types.d.ts')",
    "clean": "run-p clean:*",
    "husky-enable": "husky",
    "husky-disable": "git config --unset core.hooksPath",
    "prepublishOnly": "run-s build",
    "test:tstyche-dot": "run-s build && tstyche --target '>=5.7' --reporters ./lib/tstyche-dot-reporter.js,summary && run-s clean",
    "test:tstyche-mocha": "run-s build && tstyche --target '>=5.7' --reporters ./lib/tstyche-mocha-reporter.js,summary && run-s clean",
    "test:tstyche": "run-s build && tstyche --target '>=5.5' && run-s clean",
    "test:node": "c8 --reporter=lcov --reporter=text node --test",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*"
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "@voxpelli/eslint-config": "^23.0.0",
    "@voxpelli/tsconfig": "^16.1.0",
    "c8": "^10.1.3",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "installed-check": "^9.3.0",
    "knip": "^5.64.1",
    "npm-run-all2": "^8.0.4",
    "tstyche": "^6.1.0",
    "type-coverage": "^2.29.7",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "markdown-or-chalk": "^0.2.3"
  },
  "peerDependencies": {
    "tstyche": ">=6.1.0"
  },
  "peerDependenciesMeta": {
    "tstyche": {
      "optional": true
    }
  }
}
